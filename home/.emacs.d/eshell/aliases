alias ls ls -G $*
alias ll ls -lh $*
alias la ls -lAh $*
alias l ls -lah $*
alias git git --no-pager $*
alias less view-file $1
alias ms magit-status $1
alias ffow find-file-other-window $1
alias ffof find-file-other-frame $1
alias ff find-file $1
alias ef ediff-files $*
